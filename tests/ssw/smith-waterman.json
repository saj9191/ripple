{
	"region": "us-west-2",
	"tag": "smith-waterman",
	"storage_class": "STANDARD",
	"timeout": 300,
	"log": "maccoss-log",
	"bucket": "maccoss-ssw",
	"role": "service-role/lambdaFullAccessRole",
	"functions": {
		"map-fasta": {
			"file": "map",
		  "map_bucket": "ssw-database",
			"memory_size": 128,
		  "input_key_value": "key",
		  "bucket_key_value": "target", 
		  "directories": false,
		  "ranges": false
		},
		"smith-waterman": {
			"file": "application",
			"application": "ssw",
			"memory_size": 1024,
			"program_bucket": "ssw-program"
		},
		"top-blast": {
			"file": "top",
			"format": "blast",
			"memory_size": 1024,
 			"number": 100,
			"identifier": "score"
		},
		"combine-blast-files": {
			"file": "combine_files",
			"format": "blast",
			"memory_size": 1024
		}
	},
	"pipeline": [{
		"name": "map-fasta"
	}, {
		"name": "smith-waterman"
	},{
	  "name": "top-blast"
	},{
	  "name": "combine-blast-files"
	},{
	  "name": "top-blast"
	}]
}
