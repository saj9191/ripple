{
	"input_name": "SP1.fq",
	"model": "lambda",
	"iterations": 1,
	"region": "us-west-2",
	"folder": "fastore",
	"tag": "fastore",
	"timeout": 600,
	"bucket": "maccoss-methyl",
	"storage_class": "STANDARD",
	"credential_profile": "default",
	"role": "service-role/lambdaFullAccessRole",
	"log": "maccoss-methyl-log",
	"functions": {
	  "initiate-reference": {
		  "file": "initiate",
			"memory_size": 1024,
			"trigger_bucket": "maccoss-methyl-data",
			"trigger_key": "reference.fastq"
		},
		"split-fastq": {
		  "file": "split_file",
			"format": "fastq",
			"memory_size": 1024,
			"split_size": 10000000
		},
		"pivot-fastq": {
			"file": "pivot_file",
			"format": "fastq",
			"identifier": "signature",
			"memory_size": 1024
		},
		"combine-pivot-fastq": {
			"file": "combine_files",
			"format": "pivot",
			"memory_size": 1024,
			"sort": true
		},
		"sort-fastq": {
			"file": "sort",
			"format": "fastq",
			"identifier": "signature",
			"memory_size": 1024
		},
		"combine-fastq": {
			"file": "combine_files",
			"format": "fastq",
			"memory_size": 1024,
			"sort": false
		},
		"compress-fastore": {
			"file": "application",
			"format": "fastq",
			"application": "compress_fastore",
			"program_bucket": "maccoss-methyl-data",
			"memory_size": 3008
		},
	  "initiate-input": {
		  "file": "initiate",
			"memory_size": 1024
		}
	},
	"pipeline": [{
		"name": "initiate-reference"
	}, {
		"name": "split-fastq",
		"ranges": false
	}, {
		"name": "pivot-fastq"
	}, {
		"name": "combine-pivot-fastq"
	}, {
    "name": "split-fastq",
		"ranges": true
	}, {
		"name": "sort-fastq"
	}, {
		"name": "combine-fastq",
		"output": "reference"
	}, {
	  "name": "initiate-input",
		"input": "input"
  }, {
		"name": "split-fastq",
		"ranges": true,
		"input": "input"
	}, {
		"name": "sort-fastq"
	}, {
		"name": "combine-fastq"
	}, {
		"name": "compress-fastore",
		"input": "reference"
	}]
}
