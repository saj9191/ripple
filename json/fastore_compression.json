{
	"input_name": "SP1.fq",
	"model": "lambda",
	"iterations": 1,
	"region": "us-west-2",
	"folder": "fastore",
	"tag": "fastore",
	"timeout": 600,
	"num_bins": 50,
	"bucket": "maccoss-methyl-log",
	"storage_class": "STANDARD",
	"credential_profile": "default",
	"role": "service-role/lambdaFullAccessRole",
	"log": "maccoss-methyl-log",
	"functions": {
		"sort-fastq": {
			"file": "sort",
			"chunk_size": 1000,
			"format": "fastq",
			"identifier": "signature",
			"memory_size": 1024,
			"combine":false
		},
		"compress-fastore": {
			"file": "application",
			"format": "fastq",
			"application": "compress_fastore",
			"program_bucket": "jessie-fastore-program",
			"memory_size": 1024,
			"action": "compress"
		}
	},
	"pipeline": [{
		"name": "sort-fastq"
	}, {
		"name": "compress-fastore"
	}
	]
}
