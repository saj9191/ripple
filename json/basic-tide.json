{
  "bucket": "maccoss-tide",
  "log": "maccoss-log",
  "timeout": 600,
  "functions": {
    "split-file": {
      "file": "split_file",
      "memory_size": 128,
      "formats": [],
      "imports": []
    },
    "tide": {
      "application": "tide",
      "file": "application",
      "input_format": "mzML",
      "memory_size": 3008,
      "output_format": "tsv",
      "formats": [
        "mzML"
      ],
      "imports": []
    },
    "combine-tsv-files": {
      "file": "combine_files",
      "formats": [
        "tsv",
        "new_line"
      ],
      "memory_size": 256,
      "output_format": "tsv",
      "imports": []
    },
    "percolator": {
      "application": "percolator",
      "file": "application",
      "input_format": "tsv",
      "memory_size": 3008,
      "output_format": null,
      "formats": [
        "tsv",
        "new_line"
      ],
      "imports": []
    }
  },
  "pipeline": [
    {
      "split_size": 100000000,
      "name": "split-file",
      "output_function": "tide"
    },
    {
      "database_bucket": "maccoss-fasta",
      "num_threads": 0,
      "species": "normalHuman",
      "name": "tide",
      "output_function": "combine-tsv-files"
    },
    {
      "sort": false,
      "name": "combine-tsv-files",
      "output_function": "percolator"
    },
    {
      "database_bucket": "maccoss-fasta",
      "max_train": 10000,
      "output": "peptides",
      "name": "percolator"
    }
  ],
  "region": "us-west-2",
  "role": "service-role/lambdaFullAccessRole"
}