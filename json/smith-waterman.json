{
	"input_name": "input-10.fasta",
	"region": "us-west-1",
	"tag": "smith-waterman",
	"storage_class": "STANDARD",
	"timeout": 300,
	"log": "maccoss-log-west-1",
	"bucket": "maccoss-ssw-west-1",
	"role": "service-role/lambdaFullAccessRole",
	"sample_input": true,
	"sample_bucket": "ssw-input-west-1",
	"functions": {
		"map-fasta": {
			"file": "map",
		  "map_bucket": "ssw-database-west-1",
			"memory_size": 128
		},
		"smith-waterman": {
			"file": "application",
			"application": "ssw",
			"memory_size": 1024,
			"program_bucket": "ssw-program"
		},
		"top-blast": {
			"file": "top",
			"format": "blast",
			"memory_size": 1024,
			"chunk_size": 100000,
 			"number": 100,
			"identifier": "score"
		},
		"combine-blast-files": {
			"file": "combine_files",
			"format": "blast",
			"chunk_size": 100000,
			"num_bins": 1,
			"memory_size": 1024
		}
	},
	"pipeline": [{
		"name": "map-fasta",
		"input_key_value": "key",
		"bucket_key_value": "target", 
		"output_function": "smith-waterman",
		"directories": false,
		"ranges": false
	}, {
		"name": "smith-waterman"
	},{
	  "name": "top-blast"
	},{
	  "name": "combine-blast-files"
	},{
	  "name": "top-blast"
	}]
}
