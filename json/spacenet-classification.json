{
	"bucket": "maccoss-tide",
	"log": "maccoss-log",
	"region": "us-west-2",
	"role": "service-role/lambdaFullAccessRole",
	"storage_class": "STANDARD",
	"folder": "hello",
	"tag": "spacenet",
	"timeout": 300,
	"dependencies": {
		"formats": {
			"classification": [
				"formats/knn.py",
				"formats/pixel.py",
				"formats/new_line.py"
			],
			"knn": [
				"formats/new_line.py"
			],
			"pixel": [
				"formats/new_line.py"
			]
		},
		"libraries": "knn_libraries"
	},
	"functions": {
		"convert-to-pixels": {
			"application": "convert_to_pixels",
			"file": "application",
			"memory_size": 128,
		  "pixels_per_bin": 10000
		},
		"pair-train": {
			"application": "pair",
		  "file": "application",
			"memory_size": 128,
			"split_size": 200000
		},
		"knn": {
			"application": "run_knn",
		  "file": "application",
			"format": "classification",
			"memory_size": 3008,
			"k": 100
		},
		"combine-knn-files": {
			"file": "combine_files",
			"format": "knn",
			"memory_size": 3008,
			"k": 100
		},
		"draw-borders": {
			"application": "draw_borders",
			"file": "application",
			"format": "knn",
			"memory_size": 1024
		}
	},
	"pipeline": [{
	  "name": "convert-to-pixels",
	  "output": "pixel"
	}, {
	  "name": "pair-train"
	}, {
	  "name": "knn",
		"input": "pixel"
	}, {
	  "name": "combine-knn-files",
		"sort": true
	}, {
	  "name": "combine-knn-files",
		"sort": false
	}]
}
